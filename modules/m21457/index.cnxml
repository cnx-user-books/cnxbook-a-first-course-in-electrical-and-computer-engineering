                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mn>13</m:mn>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
<m:mtd>
<m:mo>...</m:mo>
</m:mtd>              
  <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mrow>
                        <m:mn>1</m:mn>
                        <m:mi>n</m:mi>
                      </m:mrow>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
              </m:mtr>
              <m:mtr>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mn>21</m:mn>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
