                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mn>11</m:mn>
                    </m:msub>
                  </m:mrow>
                </m:mtd>
                <m:mtd columnalign="left">
                  <m:mrow>
                    <m:mi>a</m:mi>
                    <m:msub>
                      <m:mi>x</m:mi>
                      <m:mn>12</m:mn>
                    </m:msub>
